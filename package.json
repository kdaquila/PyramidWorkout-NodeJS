{
  "name": "site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "run-s compile:styles start:server",
    "start:dev": "run-p watch:styles start:server",
    "start:server": "node server.js",
    "compile:atomizer": "atomizer --recursive --namespace=#atomic -c atomizer.config.js --outfile=scss/_atomic.scss ./views",
    "compile:sass": "sass scss:css",
    "compile:styles": "run-s compile:atomizer compile:sass",
    "watch:atomizer": "atomizer --recursive --namespace=#atomic -c atomizer.config.js --outfile=scss/_atomic.scss --watch=./views",
    "watch:sass": "sass --watch scss:css",
    "watch:styles:": "run-p watch:atomizer watch:sass"
  },
  "dependencies": {
    "atomizer": "^3.8.0",
    "bootstrap": "^4.6.0",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongodb": "^3.6.6",
    "morgan": "~1.9.1",
    "nodemailer": "^6.6.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.7"
  },
  "devDependencies": {}
}
