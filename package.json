{
  "name": "site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "run-s compile:styles start:server",
    "start:pm2": "pm2 start server.js",
    "start:dev": "run-p watch:styles watch:server",
    "start:server": "node server.js",
    "stop:pm2": "pm2 stop server",
    "watch:server": "nodemon server.js",
    "compile:atomizer": "atomizer --recursive --namespace=#atomic -c atomizer.config.js --outfile=scss/_atomic.scss ./views",
    "compile:sass": "sass scss:css",
    "compile:styles": "run-s compile:atomizer compile:sass",
    "watch:atomizer": "atomizer --verbose --recursive --namespace=#atomic -c atomizer.config.js --outfile=scss/_atomic.scss --watch=./views",
    "watch:sass": "sass --watch scss:css",
    "watch:styles:": "run-p watch:atomizer watch:sass",
    "initDB": "node ./manageDB/initializeDB.js" 
  },
  "dependencies": {
    "atomizer": "^3.8.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.6.0",
    "cookie-session": "^1.4.0",
    "crypto-random-string": "^4.0.0",
    "debug": "^4.3.1",
    "dotenv": "^8.6.0",
    "ejs": "^3.1.5",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongodb": "^3.6.6",
    "morgan": "~1.9.1",
    "nodemailer": "^6.6.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "printf": "^0.6.1",
    "sass": "^1.32.7",
    "validator": "^13.6.0"
  },
  "devDependencies": {}
}
